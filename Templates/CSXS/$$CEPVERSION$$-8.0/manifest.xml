<?xml version="1.0" encoding="UTF-8"?>
<!-- Keep Version at 7.0, even for CEP 8.0 -->
<ExtensionManifest 
	Version="7.0" 
	ExtensionBundleId="$$EXTENSION_BUNDLEID$$" 
	ExtensionBundleVersion="1.0.0"
	ExtensionBundleName="$$EXTENSION_BUNDLENAME$$" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="$$EXTENSION_ID$$" Version="$$EXTENSION_VERSION$$" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
$if "$$TARGETAPP$$" == "MULTIPLE"
			<Host Name="IDSN" Version="[13,99)" />
			<Host Name="AICY" Version="[13,99)" />
			<Host Name="PHSP" Version="[19,99)" />
			<Host Name="PHXS" Version="[19,99)" />
			<Host Name="ILST" Version="[22,99)" />
			<Host Name="PPRO" Version="[12,99)" />
			<Host Name="AEFT" Version="[15,99)" />
			<Host Name="PRLD" Version="[7,99)" />
			<Host Name="FLPR" Version="[13,99)" />
			<Host Name="DRWV" Version="[18,99)" />
			<Host Name="KBRG" Version="[9.0,99)" />
$endif
$if "$$TARGETAPP$$" == "IDSN"
			<Host Name="$$TARGETAPP$$" Version="[13,99)" />
$endif			
$if "$$TARGETAPP$$" == "AICY"
			<Host Name="$$TARGETAPP$$" Version="[13,99)" />
$endif			
$if "$$TARGETAPP$$" == "PHSP"
			<Host Name="$$TARGETAPP$$" Version="[19,99)" />
			<Host Name="PHXS" Version="[19,99)" />
$endif			
$if "$$TARGETAPP$$" == "ILST"
			<Host Name="$$TARGETAPP$$" Version="[22,99)" />
$endif			
$if "$$TARGETAPP$$" == "PPRO"
			<Host Name="$$TARGETAPP$$" Version="[12,99)" />
$endif			
$if "$$TARGETAPP$$" == "AEFT"
			<Host Name="$$TARGETAPP$$" Version="[15,99)" />
$endif			
$if "$$TARGETAPP$$" == "PRLD"
			<Host Name="$$TARGETAPP$$" Version="[7,99)" />
$endif			
$if "$$TARGETAPP$$" == "FLPR"
			<Host Name="$$TARGETAPP$$" Version="[13,99)" />
$endif			
$if "$$TARGETAPP$$" == "DRWV"
			<Host Name="$$TARGETAPP$$" Version="[18,99)" />
$endif			
$if "$$TARGETAPP$$" == "KBRG"
			<Host Name="$$TARGETAPP$$" Version="[9.0,99)" />
$endif			
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<!-- Keep Version at 7.0, even for CEP 8.0 -->
			<RequiredRuntime Name="CSXS" Version="7.0" />
		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		<Extension Id="$$EXTENSION_ID$$">
			<DispatchInfo>
				<Resources>
					<MainPath>./html/index.html</MainPath>
					<ScriptPath>./jsx/hostscript.jsx</ScriptPath>		
					<CEFCommandLine>
						<Parameter>--allow-file-access</Parameter>
						<Parameter>--allow-file-access-from-files</Parameter>
						<Parameter>--enable-nodejs</Parameter>
						<Parameter>--mixed-context</Parameter>
					</CEFCommandLine>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
$if "$$TARGETAPP$$" == "KBRG"
					<Type>ModalDialog</Type>
$else
					<Type>$$UI_TYPE$$</Type>
$endif			
					<Menu>$$EXTENSION_MENUNAME$$</Menu>
					<Geometry>
						<Size>
							<Height>$$PANELHEIGHT$$</Height>
							<Width>$$PANELWIDTH$$</Width>
						</Size>
						<MinSize>
							<Height>$$PANELHEIGHT$$</Height>
							<Width>$$PANELWIDTH$$</Width>
						</MinSize>
						<MaxSize>
							<Height>$$PANELHEIGHT$$</Height>
							<Width>$$PANELWIDTH$$</Width>
						</MaxSize>
					</Geometry>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>
