    <script>
    function handleImageClick(evt) {
        
        var path = window.location.origin;

        var src = evt.getAttribute('src');
        path += src;

        var message = {
            opcode: "insertImage",
            path: path
        };

        var messageJSON = JSON.stringify(message);

        window.top.postMessage(messageJSON, "*");
    }
    </script>
